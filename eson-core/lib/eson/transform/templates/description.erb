# See documentation <%= documentation %>

module Eson
  module Shared
    module <%= top_level_name %>
      module <%= module_name %>
        include Eson::API::DSL

        request_methods <%= request_methods %>

        url do
          set_base_path '<%= url.path %>'
<% for path in url.paths %>          path '<%= path %>'
<% end %>
<% for part in url.render_parts %>          <%= part %>
<% end %>
          <% unless url.params.empty? %>params do
<% for param in url.render_params %>            <%= param %><% end %>          end<% end %>
        end
      end
    end
  end
end
