# See documentation <%= documentation %>

module Eson
  module Shared
    module <%= top_level_name %>
      module <%= module_name %>
        extend Eson::API

        <% unless parameters.empty? %>
        # Define all parameters allowed to API endpoint
        <% for parameter in parameters %><%= parameter %>
        <% end %>
        <% end %>
      end
    end
  end
end
