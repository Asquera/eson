# See documentation <%= documentation %>

module Eson
  module Shared
    module <%= top_level_name %>
      module <%= module_name %>
        extend Eson::API

        methods <%= request_methods %>

        url '<%= url.path %>' do
          <% for path in url.paths %>path '<%= path %>'
          <% end %>
          <% for part in url.render_parts %><%= part %>
          <% end %>
          <% unless url.params.empty? %>params do
            <% for param in url.render_params %><%= param %>
            <% end %>
          end
          <% end %>
        end
      end
    end
  end
end
